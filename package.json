{
  "name": "transittrack-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "cities/*",
    "Landing Page"
  ],
  "scripts": {
    "dev:nyc": "pnpm --filter @transittrack/nyc dev",
    "dev:toronto": "pnpm --filter @transittrack/toronto dev",
    "build:nyc": "pnpm --filter @transittrack/nyc build",
    "build:toronto": "pnpm --filter @transittrack/toronto build",
    "start:nyc": "pnpm --filter @transittrack/nyc start",
    "start:toronto": "pnpm --filter @transittrack/toronto start"
  }
}